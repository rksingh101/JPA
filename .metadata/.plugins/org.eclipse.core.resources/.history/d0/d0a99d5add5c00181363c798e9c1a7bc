package example.jpa;

import javax.persistence.Entity;
import javax.persistence.Id;

@Entity
public class Address {
    @Id
	private int id;
	private String line;
	
	public Address() {
		super();
		// TODO Auto-generated constructor stub
	}

	public Address(int id, String line) {
		super();
		this.id = id;
		this.line = line;
	}
	
}
